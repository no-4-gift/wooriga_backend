<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.webapp.wooriga.mybatis.mapper.RegisteredDatesMapper">
    <insert id = "insertRegisteredDates" parameterType="RegisteredDates">
        insert into registereddates values(#{registeredIdFK},date_add(#{registeredDate}, interval +1 day))
    </insert>
</mapper>