<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.webapp.wooriga.mybatis.mapper.RegisteredChallengesMapper">

    <insert id="insertRegisteredChallenge" parameterType="RegisteredChallenges">
        insert into registeredchallenges (chiefIdFK,participantsIdFK,challengeIdFK,registeredDate,resolution) values(#{chiefIdFK},#{participantsIdFK},#{challengedIdFK},#{registeredDate},#{resolution})
    </insert>


</mapper>