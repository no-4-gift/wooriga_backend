<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.webapp.wooriga.mybatis.challenge.mapper.ChallengeImagesMapper">
    <select id = "selectChallengeId" parameterType="int" resultType="ChallengeImages">
        select image from challengeImages where challenge_id = #{challengeId} and page_id = #{pageId}
    </select>
</mapper>